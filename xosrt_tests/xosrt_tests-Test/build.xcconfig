MOE_APPLICATION_OAT=-sectcreate __OATDATA __oatdata "${SRCROOT}/build/moe/test/xcode/${CONFIGURATION}${EFFECTIVE_PLATFORM_NAME}/${arch}.oat"
MOE_APPLICATION_ART=-sectcreate __ARTDATA __artdata "${SRCROOT}/build/moe/test/xcode/${CONFIGURATION}${EFFECTIVE_PLATFORM_NAME}/${arch}.art"
MOE_APPLICATION_PROTECTION_FLAGS[sdk=iphoneos*]=-segprot __OATDATA rx rx -segprot __ARTDATA rw rw
MOE_APPLICATION_PROTECTION_FLAGS[sdk=iphonesimulator*]=-segprot __OATDATA rwx rx -segprot __ARTDATA rwx rw
MOE_OTHER_LDFLAGS_SIM=${MOE_APPLICATION_OAT} ${MOE_APPLICATION_ART} ${MOE_APPLICATION_PROTECTION_FLAGS} -lstdc++ -pagezero_size 4096
MOE_OTHER_LDFLAGS_DEV=${MOE_APPLICATION_OAT} ${MOE_APPLICATION_ART} ${MOE_APPLICATION_PROTECTION_FLAGS} -lstdc++
ENABLE_BITCODE=NO
